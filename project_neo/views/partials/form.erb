<form action="/neo/<%= $neo_pull.neo_ex_id %>" method="GET">
  <div class="inputBox">
    <label>ID</label>
    <input type="search" placeholder="Enter an ID" value="<%= @neo %>">
  </div>
  <div class="inputBox">
    <input type="submit" value="Show">
  </div>

  <% if ($neo_pull.neo_ex_id) %>
    <input type="hidden" name="_method" value="GET">
  <% end %>
</form>
