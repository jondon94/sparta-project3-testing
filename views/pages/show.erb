<div class="">
  <h3>Asteroid Name: <%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['name'] %></h3>
  <span>Link to the NASA JPL Database for: <a href="<%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['nasa_jpl_url'] %>" id="jpl_link"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['name'] %></a></span>
</div>

<span>---------------------------------------------------------------------------------------------------------------------------</span>
<table>
  <tr>
    <th class="tab_col">Estimated Diameter</th>
    <th class="tab_col">Minimum Estimated Diameter</th>
    <th class="tab_col">Maximum Estimated Diameter</th>
  </tr>
  <tr>
    <td class="tab_col">Kilometers</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['kilometers']['estimated_diameter_min'] %></td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['kilometers']['estimated_diameter_max'] %></td>
  </tr>
  <tr>
    <td class="tab_col">Meters</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['meters']['estimated_diameter_min'] %></td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['meters']['estimated_diameter_max'] %></td>
  </tr>
  <tr>
    <td class="tab_col">Feet</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['feet']['estimated_diameter_min'] %></td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['feet']['estimated_diameter_max'] %></td>
  </tr>
  <tr>
    <td class="tab_col">Miles</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['miles']['estimated_diameter_min'] %></td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['estimated_diameter']['miles']['estimated_diameter_max'] %></td>
  </tr>
</table>
<span>---------------------------------------------------------------------------------------------------------------------------</span>

<table>
  <tr>
    <th class="tab_col">Unit</th>
    <th class="tab_col">Relative Velocity</th>
  </tr>
  <tr>
    <td class="tab_col">Kilometers per Second</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['relative_velocity']['kilometers_per_second'] %></td>
  </tr>
  <tr>
    <td class="tab_col">Kilometers per Hour</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['relative_velocity']['kilometers_per_hour'] %></td>
  </tr>
  <tr>
    <td class="tab_col">Miles per Hour</td>
    <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['relative_velocity']['miles_per_hour'] %></td>
  </tr>
</table>

<span>---------------------------------------------------------------------------------------------------------------------------</span>

<h3>Absolute Magnitude: <%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['absolute_magnitude_h'] %></h3>
<h3>Potentially Hazardous: <%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['is_potentially_hazardous_asteroid'] %></h3>
<h3>Orbital Body: <%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['orbiting_body'] %></h3>

<button onclick="myFunction()">Orbital Data</button>
<div id="orbt_sec">
  <table>
    <tr>
      <td class="tab_col">Oribit ID</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['orbit_id'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Oribtal Period</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['orbital_period'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Orbit Determined Date</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['orbit_determination_date'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Uncertainty</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['orbit_uncertainty'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Minimum Orbit Intersection</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['minimum_orbit_intersection'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Jupiter Tisserand Invariant</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['jupiter_tisserand_invariant'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Eccentricity of Orbit</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['eccentricity'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Semi-Major Axis</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['orbital_data']['semi_major_axis'] %></td>
    </tr>
    <tr>
      <td>Date NEO Closest Approach</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['close_approach_date'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Miss Distance A.U.</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['miss_distance']['astronomical'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Miss Distance L.U.</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['miss_distance']['lunar'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Miss Distance Kilometers</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['miss_distance']['kilometers'] %></td>
    </tr>
    <tr>
      <td class="tab_col">Miss Distance Miles</td>
      <td class="tab_col"><%= $neo_brws.brws_neo_id(@pgn)['near_earth_objects'][@id]['close_approach_data'][0]['miss_distance']['miles'] %></td>
    </tr>
  </table>
  </div>

<script>
function myFunction() {
    var x = document.getElementById("orbt_sec");
    if (x.style.display === "none") {
        x.style.display = "block";
    } else {
      x.style.display = "none";
    }
}
</script>
